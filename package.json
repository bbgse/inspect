{
  "name": "@bbgse/loupe",
  "version": "1.0.0",
  "description": "Inspect utility for Node.js",
  "homepage": "https://github.com/bbgse/loupe",
  "license": "MIT",
  "author": "Veselin Todorov <hi@vesln.com>",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bbgse/inspect.git"
  },
  "files": [
    "inspect.js",
    "lib/*"
  ],
  "scripts": {
    "test": "vitest",
    "build": "npm run build:lib && npm run build:esm-bundle && npm run build:cjs-bundle",
    "build:lib": "esbuild \"src/*.ts\" --outdir=lib --format=esm",
    "build:esm-bundle": "esbuild --bundle src/index.ts --outfile=inspect.js --format=esm",
    "build:cjs-bundle": "esbuild --bundle src/index.ts --outfile=inspect.js --format=cjs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "get-func-name": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "esbuild": "^0.19.5",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
